    "@types/express": "^4.11.1",
    "@types/faker": "^4.1.2",
    "@types/jest": "^22.2.3",
    "@types/js-cookie": "^2.1.0",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/lodash": "^4.14.109",
    "@types/next": "^2.4.10",
    "@types/next-redux-wrapper": "^1.4.1",
    "@types/node": "^8.9.4",
    "@types/passport": "^0.4.3",
