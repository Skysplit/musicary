{
  "name": "expressjs-typescript-app",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "cli": "NODE_ENV=production node -r ./dist/bootstrap",
    "cli:dev": "ts-node -P ./tsconfig.build.json -r ./src/bootstrap",
    "serve": "NODE_ENV=production node dist/index.js",
    "start": "npm run serve",
    "start:dev": "nodemon ./src/index.ts",
    "start:debug": "nodemon --inspect ./src/index.ts",
    "build:clean": "rimraf dist",
    "build:server": "tsc -p ./tsconfig.build.json",
    "build:client": "next build src/client",
    "build": "npm run build:clean && concurrently -n 'Server,Client' 'npm run build:server' 'npm run build:client'",
    "lint": "tslint --format stylish --project ./tsconfig.json --config ./tslint.json",
    "test": "npm run lint && npm run test:ci",
    "test:ci": "npm run test:client:ci && npm run test:server:ci",
    "test:server": "cross-env NODE_ENV=testing jest --config jest.config.server.js --runInBand",
    "test:server:watch": "npm run test:server -- --watch ",
    "test:server:ci": "npm run test:server -- --forceExit --coverage --verbose",
    "test:client": "cross-env NODE_ENV=testing jest --config jest.config.client.js",
    "test:client:watch": "npm run test:client -- --watch",
    "test:client:ci": "npm run test:client -- --forceExit --coverage --verbose",
    "typeorm": "npm run cli -- node_modules/.bin/typeorm",
    "typeorm:dev": "npm run cli:dev -- node_modules/.bin/typeorm",
    "heroku-postbuild": "npm run build"
  },
  "author": "Tomasz Romik",
  "license": "MIT",
  "engines": {
    "node": "~8.9.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.2",
    "class-validator": "^0.8.5",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "dotenv": "^5.0.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "express-validator": "^5.2.0",
    "faker": "^4.1.0",
    "formik": "^0.11.11",
    "googleapis": "^30.0.0",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.10",
    "mysql": "^2.15.0",
    "next": "^6.0.3",
    "next-redux-wrapper": "^2.0.0-beta.6",
    "next-routes": "^1.4.2",
    "node-sass": "^4.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "re-reselect": "^2.1.0",
    "react": "^16.3.2",
    "react-beautiful-dnd": "^7.1.3",
    "react-dom": "^16.3.2",
    "react-md": "^1.3.1",
    "react-player": "^1.6.1",
    "react-redux": "^5.0.7",
    "recompose": "^0.27.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "reflect-metadata": "^0.1.12",
    "reselect": "^3.0.1",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
